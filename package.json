{
  "name": "formbuilder",
  "version": "0.0.1",
  "description": "A tool to build forms on the web",
  "scripts": {
    "build:formbuilder": "NODE_ENV=production rimraf build && webpack --config webpack.config.prod.js --optimize-minimize && cp public/index.prod.html build/index.html",
    "build:formbuilder-gh": "rimraf build && webpack --config webpack.config.github.js && cp public/index.prod.html build/index.html",
    "lint": "eslint src test",
    "publish-to-gh-pages": "npm run build:formbuilder-gh && gh-pages --dist build/",
    "publish-to-npm": "npm run dist && npm publish",
    "start": "react-scripts start",
    "tdd": "npm run test -- -w",
    "test": "jest"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "@rjsf/core": "^2.5.1",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-drag-and-drop": "^2.0.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^1.0.3",
    "string": "^3.3.1",
    "urlencode": "^1.1.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "css-loader": "^0.23.1",
    "depcheck": "^1.4.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "html": "^1.0.0",
    "jsdom": "^7.2.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^8.3.2",
    "react-scripts": "^4.0.3",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.4.4",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1"
  },
  "directories": {
    "test": "test"
  },
  "author": "Micha≈Ç Bargiel <michal5bargiel@gmail.com>",
  "keywords": [
    "react",
    "form",
    "json-schema"
  ],
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.js$": [
        "ts-jest"
      ]
    }
  }
}
